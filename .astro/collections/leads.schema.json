{
  "$ref": "#/definitions/leads",
  "definitions": {
    "leads": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "dateOfBirth": {
          "type": "string"
        },
        "gender": {
          "type": "string",
          "enum": [
            "male",
            "female",
            "non-binary",
            "prefer-not-to-say"
          ]
        },
        "phone": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "address": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "zipCode": {
          "type": "string"
        },
        "emergencyContactName": {
          "type": "string"
        },
        "emergencyContactPhone": {
          "type": "string"
        },
        "emergencyContactRelationship": {
          "type": "string"
        },
        "insuranceProvider": {
          "type": "string"
        },
        "insuranceID": {
          "type": "string"
        },
        "paymentMethod": {
          "type": "string",
          "enum": [
            "insurance",
            "self-pay",
            "family",
            "other"
          ]
        },
        "previousTreatment": {
          "type": "string"
        },
        "currentSobrietyDate": {
          "type": "string"
        },
        "substancesUsed": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": []
        },
        "lastUseDate": {
          "type": "string"
        },
        "detoxCompleted": {
          "type": "boolean",
          "default": false
        },
        "detoxLocation": {
          "type": "string"
        },
        "message": {
          "type": "string"
        },
        "currentLiving": {
          "type": "string"
        },
        "transportation": {
          "type": "boolean",
          "default": false
        },
        "employment": {
          "type": "string"
        },
        "legalIssues": {
          "type": "boolean",
          "default": false
        },
        "medicalIssues": {
          "type": "string"
        },
        "preferredFacility": {
          "type": "string"
        },
        "preferredProgram": {
          "type": "string"
        },
        "moveInDate": {
          "type": "string"
        },
        "lengthOfStay": {
          "type": "string"
        },
        "source": {
          "type": "string"
        },
        "referredBy": {
          "type": "string"
        },
        "status": {
          "type": "string",
          "enum": [
            "new",
            "contacted",
            "scheduled",
            "qualified",
            "admitted",
            "declined",
            "lost"
          ],
          "default": "new"
        },
        "priority": {
          "type": "string",
          "enum": [
            "high",
            "medium",
            "low"
          ],
          "default": "medium"
        },
        "createdAt": {
          "type": "string"
        },
        "lastContactDate": {
          "type": "string"
        },
        "nextFollowUp": {
          "type": "string"
        },
        "staffAssigned": {
          "type": "string"
        },
        "notes": {
          "type": "string"
        },
        "privateNotes": {
          "type": "string"
        },
        "leadScore": {
          "type": "number",
          "default": 0
        },
        "conversionProbability": {
          "type": "number",
          "default": 50
        },
        "$schema": {
          "type": "string"
        }
      },
      "required": [
        "firstName",
        "lastName",
        "dateOfBirth",
        "gender",
        "phone",
        "email",
        "emergencyContactName",
        "emergencyContactPhone",
        "emergencyContactRelationship",
        "paymentMethod",
        "message",
        "source",
        "createdAt"
      ],
      "additionalProperties": false
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}